def findgcf(x,y):
    smaller = 0
    if(x<y):
        smaller = x
    else:
        smaller = y
    gcf = 0
    for i in range(1,smaller + 1):
        if (x % i == 0) and (y % i == 0):
            gcf = i
    print(gcf)

number1=int(input())
number2=int(input())
findgcf (number1,number2)

print(f"The GCF of {number1} and {number2} is {findgcf(number1,number2)}")


  